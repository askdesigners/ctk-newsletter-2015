{
  "locals": {
    "url": "http://localhost:8080",
    "name": "CTK Newsletter 2015",
    "owner": "CTK",
    "description": "News from 2015"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "wintersmith-libsass"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 50
  },
  "node-sass": {
    "minify": false
  },
  "clean-css": {
    "keepBreaks": true,
    "removeEmpty": true
  }
}
